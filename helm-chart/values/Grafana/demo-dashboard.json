{
  "id": null,
  "title": "Spring Boot Demo Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests per Second",
      "targets": [
        {
          "expr": "sum(irate(http_server_requests_seconds_count[1m])) by (status, method)",
          "legendFormat": "{{method}} [{{status}}]"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      }
    },
    {
      "type": "timeseries",
      "title": "JVM Heap Memory Used",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "{{id}}"
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "application",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "query": "label_values(http_server_requests_seconds_count, application)"
      }
    ]
  }
}
